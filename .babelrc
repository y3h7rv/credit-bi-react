{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 2
      }
    ],
    [
      "@babel/preset-react",
      {
        "throwIfNamespace": false
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "version": "2023-05" }],
    ["@babel/plugin-proposal-class-properties", { "loose": false }]
  ]
}